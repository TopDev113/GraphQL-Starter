  "version": "0.2.0",
  "configurations": [
    {
      "name": "Docker: Attach to Nodemon",
      "type": "node",
      "request": "attach",
      "localRoot": "${workspaceRoot}/src",
      "remoteRoot": "/usr/src/app/src",
      "protocol": "inspector",
      "address": "0.0.0.0",
      "port": 9229,
      "restart": true
    }
  ]
}
