{
  "name": "graphql-starter",
  "description": "GraphQL Starter",
  "scripts": {
    "prestart": "graphql-codegen",
    "start": "concurrently \"npm run nodemon\" \"npm run generate:gql-types\" ",
    "production": "ts-node src/app.ts",
    "ts-node": "ts-node",
    "nodemon": "nodemon -L",
    "lint": "tslint -p tsconfig.json",
    "prettier": "prettier --write \"src/**/*.{js,ts,json,css,scss,md}\"",
    "migrate:make": "knex --knexfile ./knexfile.ts migrate:make -x ts",
    "migrate:latest": "knex --knexfile ./knexfile.ts migrate:latest",
    "migrate:rollback": "knex --knexfile ./knexfile.ts migrate:rollback",
    "seed:make": "knex --knexfile ./knexfile.ts seed:make -x ts",
    "seed": "knex --knexfile ./knexfile.ts seed:run",
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate:gql-types": "graphql-codegen --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerino-ligutom/GraphQL-Starter.git"
  },
  "author": {
    "email": "zeferinix@gmail.com",
    "name": "Cerino O. Ligutom III",
    "url": "https://www.zeferinix.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cerino-ligutom/GraphQL-Starter/issues"
  },
  "homepage": "https://github.com/cerino-ligutom/GraphQL-Starter#readme",
  "devDependencies": {
    "@graphql-codegen/cli": "1.4.0",
    "@graphql-codegen/introspection": "1.4.0",
    "@graphql-codegen/typescript": "1.4.0",
    "@graphql-codegen/typescript-resolvers": "1.4.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "1.0.0",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/graphql": "^14.2.3",
    "@types/graphql-depth-limit": "^1.1.0",
    "@types/graphql-iso-date": "^3.3.1",
    "@types/helmet": "0.0.43",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.6.9",
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1",
    "rimraf": "^2.6.3",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0"
  },
  "dependencies": {
    "apollo-server-express": "^2.8.1",
    "base64url": "^3.0.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dataloader": "^1.4.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "graphql": "^14.3.1",
    "graphql-depth-limit": "^1.1.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-middleware": "^3.0.3",
    "graphql-shield": "^6.0.4",
    "helmet": "^3.20.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.1",
    "lodash": "^4.17.15",
    "merge-graphql-schemas": "^1.7.0",
    "objection": "^1.6.9",
    "pg": "^7.12.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3",
    "yup": "^0.27.0"
  }
}
