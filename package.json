{
  "name": "graphql-starter",
  "description": "GraphQL Starter",
  "scripts": {
    "prestart": "graphql-codegen",
    "start": "concurrently -n \"api,codegen\" -c \"bgCyan.bold,bgMagenta.bold\" \"npm run nodemon\" \"npm run generate:gql-types\" ",
    "production": "ts-node src/app.ts",
    "ts-node": "ts-node",
    "nodemon": "nodemon -L",
    "lint": "tslint -p tsconfig.json",
    "prettier": "prettier --write \"src/**/*.{js,ts,json,css,scss,md}\"",
    "migrate:make": "knex --knexfile ./knexfile.ts migrate:make -x ts",
    "migrate:latest": "knex --knexfile ./knexfile.ts migrate:latest",
    "migrate:rollback": "knex --knexfile ./knexfile.ts migrate:rollback",
    "seed:make": "knex --knexfile ./knexfile.ts seed:make -x ts",
    "seed": "knex --knexfile ./knexfile.ts seed:run",
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate:gql-types": "graphql-codegen --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerino-ligutom/GraphQL-Starter.git"
  },
  "author": {
    "email": "zeferinix@gmail.com",
    "name": "Cerino O. Ligutom III",
    "url": "https://www.zeferinix.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cerino-ligutom/GraphQL-Starter/issues"
  },
  "homepage": "https://github.com/cerino-ligutom/GraphQL-Starter#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.12.2",
    "@graphql-codegen/introspection": "1.12.2",
    "@graphql-codegen/typescript": "1.12.2",
    "@graphql-codegen/typescript-resolvers": "1.12.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "1.7.0",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/graphql": "^14.5.0",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/graphql-iso-date": "^3.3.3",
    "@types/helmet": "0.0.45",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/lodash": "^4.14.149",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.7.0",
    "concurrently": "^5.1.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.0.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0"
  },
  "dependencies": {
    "apollo-server-express": "^2.10.0",
    "base64url": "^3.0.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-status-monitor": "^1.2.8",
    "graphql": "^14.6.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.0.11",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.9",
    "lodash": "^4.17.15",
    "merge-graphql-schemas": "^1.7.6",
    "morgan": "^1.9.1",
    "objection": "^2.1.2",
    "pg": "^7.18.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "winston": "^3.2.1",
    "yup": "^0.28.1"
  }
}
