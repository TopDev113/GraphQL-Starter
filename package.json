{
  "name": "graphql-starter",
  "description": "GraphQL Starter",
  "scripts": {
    "release": "standard-version",
    "start": "concurrently -n \"api,codegen\" -c \"bgCyan.bold,bgMagenta.bold\" \"npm run nodemon\" \"npm run generate:gql-types --watch\" ",
    "start:production": "node ./src/app.js",
    "build": "run-s build:clean build:typescript build:copy-files",
    "build:clean": "rimraf build",
    "build:typescript": "tsc && npx tsc-alias",
    "build:copy-files": "copyfiles \"src/**/!(*.ts)\" package*.json LICENSE build",
    "nodemon": "nodemon",
    "lint": "eslint ./src",
    "prettier": "prettier --write \"src/**/*.{js,ts,json,css,scss,md}\"",
    "migrate:make": "knex --knexfile ./knexfile.* migrate:make -x ts",
    "migrate:latest": "knex --knexfile ./knexfile.* migrate:latest",
    "migrate:rollback": "knex --knexfile ./knexfile.* migrate:rollback",
    "seed:make": "knex --knexfile ./knexfile.* seed:make -x ts",
    "seed": "knex --knexfile ./knexfile.* seed:run",
    "test": "echo \"No test specified\" && exit 0",
    "tsc-check": "npx tsc --noEmit",
    "generate:gql-types": "npx graphql-codegen",
    "apollo:service:push": "npx apollo service:push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerino-ligutom/GraphQL-Starter.git"
  },
  "author": {
    "email": "zeferinix@gmail.com",
    "name": "Cerino O. Ligutom III",
    "url": "https://www.zeferinix.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cerino-ligutom/GraphQL-Starter/issues"
  },
  "homepage": "https://github.com/cerino-ligutom/GraphQL-Starter#readme",
  "dependencies": {
    "@graphql-tools/load-files": "6.2.5",
    "@graphql-tools/merge": "6.2.9",
    "apollo-server-express": "2.21.0",
    "bcryptjs": "2.4.3",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dataloader": "2.0.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-status-monitor": "1.3.3",
    "graphql": "15.5.0",
    "graphql-depth-limit": "1.1.0",
    "graphql-iso-date": "3.6.1",
    "graphql-redis-subscriptions": "^2.3.1",
    "graphql-scalars": "1.7.1",
    "graphql-upload": "^11.0.0",
    "helmet": "4.4.1",
    "ioredis": "4.22.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "0.21.17",
    "npm-run-all": "4.1.5",
    "objection": "2.2.14",
    "objection-cursor": "1.2.4",
    "pg": "8.5.1",
    "tsconfig-paths": "^3.9.0",
    "uuid": "^8.3.2",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@graphql-codegen/add": "2.0.2",
    "@graphql-codegen/cli": "1.20.1",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/typescript": "1.21.0",
    "@graphql-codegen/typescript-resolvers": "1.18.2",
    "@types/bcryptjs": "2.4.2",
    "@types/compression": "1.7.0",
    "@types/cors": "2.8.10",
    "@types/express": "4.17.11",
    "@types/graphql-depth-limit": "1.1.2",
    "@types/graphql-iso-date": "3.4.0",
    "@types/graphql-upload": "^8.0.4",
    "@types/ioredis": "^4.19.4",
    "@types/node": "14.14.31",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "4.15.1",
    "@typescript-eslint/parser": "4.15.1",
    "concurrently": "6.0.0",
    "copyfiles": "2.4.1",
    "eslint": "7.20.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "husky": "5.0.9",
    "lint-staged": "10.5.4",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "standard-version": "9.1.1",
    "ts-node": "9.1.1",
    "tsc-alias": "^1.2.6",
    "typescript": "4.1.5"
  }
}
